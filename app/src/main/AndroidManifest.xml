<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iuliapopa.bac_androidapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_action_book"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Date_Elev"
            android:label="@string/title_activity_date__elev"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".AdaugareElev"
            android:label="@string/title_activity_adaugare_elev"
            android:theme="@style/AppTheme"></activity>

        <activity android:name=".com.example.activity.LiceuActivity"/>
        <activity android:name=".com.example.activity.DisciplinaActivity"/>
        <activity android:name=".com.example.activity.ProfilActivity"/>
        <activity android:name=".com.example.activity.SpecializareActivity"/>
    </application>

</manifest>